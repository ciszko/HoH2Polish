	<string name="skills.attacks.mainhand.mace.name">Mace Bash</string>
	<string name="skills.attacks.mainhand.mace.desc">Potezny zamach, ktory zadaje 100% obrazen od broni i ma 15% szans na zadanie \cb0dfffStun\d.</string>
	<string name="skills.attacks.mainhand.axe.name">Axe Slash</string>
	<string name="skills.attacks.mainhand.axe.desc">Rozdzierajace ciecie, ktore zadaje 100% obrazen od broni i ma 20% szans na zadanie stosu \cb0dfffBleeding\d.</string>
	<string name="skills.attacks.mainhand.sword.name">Sword Swing</string>
	<string name="skills.attacks.mainhand.sword.desc">Szeroki zamach po luku zadajacy 100% obrazen od broni.</string>
	<string name="skills.attacks.mainhand.dagger.name">Ciecie sztyletem</string>
	<string name="skills.attacks.mainhand.dagger.desc">Szybkie ciecie, ktore zadaje 100% obrazen od broni i ma 20% szans na zadanie stosu \cb0dfffBleeding\d.</string>
	<string name="skills.attacks.mainhand.dirk.name">Dirk Stab</string>
