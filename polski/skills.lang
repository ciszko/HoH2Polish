	<string name="skills.rogue.mastery_3.desc"></string>

	<string name="skills.rogue.mastery_3.spell_1.name">Granat</string>
	<string name="skills.rogue.mastery_3.spell_1.desc">Rzucasz granat przed siebie, który po krótkim czasie wybucha, zadając obrażenia fizyczne.</string>
	<string name="skills.rogue.mastery_3.spell_1.up1.name">Granaty zapalające</string>
	<string name="skills.rogue.mastery_3.spell_1.up1.desc">Granat eksploduje teraz przy uderzeniu i pozostawia obszar płonącej ziemi na krótki czas, zadając obrażenia od ognia.</string>
	<string name="skills.rogue.mastery_3.spell_1.up2.name">Granaty wstrząsowe</string>
	<string name="skills.rogue.mastery_3.spell_1.up2.desc">Granat zadaje dodatkowe obrażenia od błyskawic, które mogą zostać połączone z innymi pobliskimi wrogami.</string>
	<string name="skills.rogue.mastery_3.spell_1.up3.name">Wybuchowe wyjście</string>
	<string name="skills.rogue.mastery_3.spell_1.up3.desc">Jeśli rozpoczniesz skok w pobliżu wroga, upuścisz granat. Efekt ten ma krótki czas odnowienia.</string>
	
	<string name="skills.rogue.mastery_3.passive_1.name">Kombinezon rzemieślnika</string>
	<string name="skills.rogue.mastery_3.passive_1.desc">Zwiększa pancerz i obrażenia fizyczne. Otrzymanie obrażeń powoduje aktywację błyskawicy, która poraża pobliskich wrogów \cb0dfffŚlepotą\d. Efekt ten ma krótki czas odnowienia</string>
	<string name="skills.rogue.mastery_3.passive_1.up1.name">Zzzap</string>
	<string name="skills.rogue.mastery_3.passive_1.up1.desc">Umiejętność \cffdfb0Weapon\d ma szansę aktywować błyskawicę, która porazi losowego pobliskiego wroga, zadając mu obrażenia od błyskawic.</string>
	<string name="skills.rogue.mastery_3.passive_1.up2.name">Odłamek</string>
	<string name="skills.rogue.mastery_3.passive_1.up2.desc">Rzucanie zaklęć ma szansę wyrzucić wiele odłamków we wszystkich kierunkach, zadając obrażenia fizyczne i od ognia.</string>
	<string name="skills.rogue.mastery_3.passive_1.up3.name"></string>
	<string name="skills.rogue.mastery_3.passive_1.up3.desc"></string>

