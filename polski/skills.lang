	<string name="skills.attacks.twohanded.maul1.name">Maul Swing</string>
	<string name="skills.attacks.twohanded.maul1.desc">Powolny, ciężki zamach, który zadaje 150% obrażeń od broni i ma 20% szans na zadanie \cb0dfffStun\d.</string>
	<string name="skills.attacks.twohanded.maul2.name">Ground Slam</string>
	<string name="skills.attacks.twohanded.maul2.desc">Potężne uderzenie o ziemię, które zadaje 200% obrażeń obszarowych od broni i powoduje \cb0dfffStun\d.</string>

	<string name="skills.attacks.twohanded.greataxe1.name">Greataxe Slash</string>
	<string name="skills.attacks.twohanded.greataxe1.desc">Szerokie, rozdzierające cięcie, które zadaje 125% obrażeń od broni i ma 20% szans na zadanie stosu \cb0dfffBleeding\d.</string>
	<string name="skills.attacks.twohanded.greataxe2.name">Whirlwind</string>
	<string name="skills.attacks.twohanded.greataxe2.desc">Przytrzymaj, aby nieustannie obracać toporem, zadając 75% obrażeń od broni wszystkim wrogom w okolicy.</string>

	<string name="skills.attacks.twohanded.daggers1.name">Dual Slice</string>
	<string name="skills.attacks.twohanded.daggers1.desc">Wykonaj dwa uderzenia w krótkim odstępie czasu, a każde z nich zada 100% obrażeń fizycznych.</string>
	<string name="skills.attacks.twohanded.daggers2.name">Lunging Strike</string>
	<string name="skills.attacks.twohanded.daggers2.desc">Szybki wypad do przodu, który uderza wszystkich wrogów na twojej drodze, zadając 200% obrażeń od broni.</string>

	<string name="skills.attacks.twohanded.staff.fire1.name">Flame Spray</string>
	<string name="skills.attacks.twohanded.staff.fire1.desc">Przytrzymaj, aby nieustannie tryskać płomieniami, zadając 25% obrażeń od broni.</string>
	<string name="skills.attacks.twohanded.staff.fire2.name">Ognisty podmuch</string>
	<string name="skills.attacks.twohanded.staff.fire2.desc">Uderz laską w ziemię, aby wyzwolić ognisty podmuch zadający 100% obrażeń od broni na danym obszarze.</string>

	<string name="skills.attacks.twohanded.staff.ice1.name">Lodowa Lanca</string>
	<string name="skills.attacks.twohanded.staff.ice1.desc">Przytrzymaj, aby naładować lodowy pocisk. Zasięg, prędkość i obrażenia pocisku zwiększają się o czas ładowania, zadając do 75% obrażeń od broni.</string>
	<string name="skills.attacks.twohanded.staff.ice2.name">Lodowe kolce</string>
	<string name="skills.attacks.twohanded.staff.ice2.desc">Uderz laską w ziemię, aby spowodować wybuch trzech kolejnych lodowych kolców w kierunku do przodu, z których każdy zadaje 150% obrażeń od broni.</string>

	<string name="skills.attacks.twohanded.staff.lightning1.name">Lightning Beam</string>
	<string name="skills.attacks.twohanded.staff.lightning1.desc">Przytrzymaj, aby skierować wiązkę elektryczności, która zadaje 40% obrażeń od broni. Wiązka traci 10% obrażeń z każdym kolejnym trafionym wrogiem.</string>
	<string name="skills.attacks.twohanded.staff.lightning2.name">Thunderbolts</string>
	<string name="skills.attacks.twohanded.staff.lightning2.desc">Uderz laską w ziemię, aby wyczarować dwa pioruny, które uderzą w obszar przed tobą, zadając po 100% obrażeń od broni.</string>



	<!-- Paladin -->


	<string name="skills.paladin.spell_1.name">Błogosławione uderzenie</string>
	<string name="skills.paladin.spell_1.desc">Uderzasz wszystkich wrogów na obszarze przed tobą, zadając obrażenia fizyczne.</string>
	<string name="skills.paladin.spell_1.up1.name">Massive Strike</string>
	<string name="skills.paladin.spell_1.up1.desc">Zwiększa rozmiar obszaru rażenia.</string>
	<string name="skills.paladin.spell_1.up2.name">Płomienie oczyszczające</string>
	<string name="skills.paladin.spell_1.up2.desc">Obszar trafienia staje w płomieniach, zadając przez pewien czas dodatkowe obrażenia od ognia.</string>
	<string name="skills.paladin.spell_1.up3.name">Gniewne Uderzenie</string>
	<string name="skills.paladin.spell_1.up3.desc">Zadaje dodatkowe obrażenia od błyskawic, które mogą zostać połączone z innymi pobliskimi wrogami.</string>

	<string name="skills.paladin.spell_2.name">Nakładanie rąk</string>
	<string name="skills.paladin.spell_2.desc">Uzdrawiasz siebie i wszystkich sojuszników wokół ciebie.</string>
	<string name="skills.paladin.spell_2.up1.name">Ożywczy dotyk</string>
	<string name="skills.paladin.spell_2.up1.desc">Stosuje również wzmocnienie, które zwiększa regenerację zdrowia i many na krótki czas.</string>
	<string name="skills.paladin.spell_2.up2.name">Oczyszczający dotyk</string>
	<string name="skills.paladin.spell_2.up2.desc">Usuwa wszystkie osłabienia i afekty. Zyskujesz również odporność na wszystkie osłabienia na krótki czas.</string>
	<string name="skills.paladin.spell_2.up3.name">Zmień się w nieumarłego</string>
	<string name="skills.paladin.spell_2.up3.desc">Zadaje obrażenia fizyczne wszystkim nieumarłym wrogom w okolicy.</string>

	<string name="skills.paladin.passive_1.name">Pancerny rycerz</string>
	<string name="skills.paladin.passive_1.desc">Zwiększa pancerz i daje szansę na zablokowanie 100% obrażeń fizycznych.</string>
	<string name="skills.paladin.passive_1.up1.name">Elemental Bulwark</string>
	<string name="skills.paladin.passive_1.up1.desc">Zwiększa odporność na ogień, lód i błyskawice.</string>
	<string name="skills.paladin.passive_1.up2.name">Błyskawiczne ciosy</string>
	<string name="skills.paladin.passive_1.up2.desc">Zyskujesz dodatkową szansę na zablokowanie 100% obrażeń dowolnego typu.</string>
	<string name="skills.paladin.passive_1.up3.name">Zbroja Gniewu</string>
	<string name="skills.paladin.passive_1.up3.desc">Otrzymując obrażenia, masz szansę trafić losowego wroga w pobliżu błyskawicą, zadając obrażenia od błyskawic zależne od wartości twojego pancerza.</string>

	<string name="skills.paladin.passive_2.name">Wyrok skazujący</string>
	<string name="skills.paladin.passive_2.desc">Trafienia bronią do walki wręcz mają szansę przywrócić ci część many.</string>
	<string name="skills.paladin.passive_2.up1.name">Rozwiązanie</string>
	<string name="skills.paladin.passive_2.up1.desc">Gdy masz mniej niż połowę zdrowia, twoje zaklęcia kosztują mniej many.</string>
	<string name="skills.paladin.passive_2.up2.name">Tarcza Wiary</string>
	<string name="skills.paladin.passive_2.up2.desc">Gdy pozostało ci więcej niż połowa maksymalnej many, blokujesz obrażenia za koszt many.</string>
	<string name="skills.paladin.passive_2.up3.name">Zeal</string>
	<string name="skills.paladin.passive_2.up3.desc">Kiedy aktywuje się \cb0dfffConviction\d, zadajesz dodatkowe obrażenia fizyczne zależne od twojej aktualnej many.</string>


	<string name="skills.paladin.mastery_1.name">Sprawiedliwość</string>
	<string name="skills.paladin.mastery_1.desc"></string>

	<string name="skills.paladin.mastery_1.spell_1.name">Wyrok</string>
	<string name="skills.paladin.mastery_1.spell_1.desc">Przez pewien czas osądzasz losowych wrogów w pobliżu, uderzając w nich boskim młotem i zadając im obrażenia fizyczne.</string>
	<string name="skills.paladin.mastery_1.spell_1.up1.name">Przedłużające się ściganie</string>
	<string name="skills.paladin.mastery_1.spell_1.up1.desc">Gdy \cb0dfffJudgement\d jest aktywne, zyskujesz szansę na wydłużenie czasu jego trwania podczas zabijania wrogów. Czas trwania nie może zostać wydłużony do więcej niż 7 sekund.</string>
	<string name="skills.paladin.mastery_1.spell_1.up2.name">Młot Gniewu</string>
	<string name="skills.paladin.mastery_1.spell_1.up2.desc">Zadaje dodatkowe obrażenia od błyskawic.</string>
	<string name="skills.paladin.mastery_1.spell_1.up3.name"></string>
	<string name="skills.paladin.mastery_1.spell_1.up3.desc"></string>

	<string name="skills.paladin.mastery_1.passive_1.name">Lawbringer</string>
	<string name="skills.paladin.mastery_1.passive_1.desc">Kiedy blokujesz lub otrzymujesz obrażenia, masz szansę zadać atakującemu \cb0dfffDisarm\d i \cb0dfffSilence\d.</string>
	<string name="skills.paladin.mastery_1.passive_1.up1.name">Karać niegodziwców</string>
	<string name="skills.paladin.mastery_1.passive_1.up1.desc">Zadajesz większe obrażenia wrogom, którym zadano \cb0dfffDisarm\d lub \cb0dfffSilence\d.</string>
	<string name="skills.paladin.mastery_1.passive_1.up2.name">Szekle</string>
	<string name="skills.paladin.mastery_1.passive_1.up2.desc">Trafienia \cffdfb0Bronią\d i \cffdfb0Zaklęciem\d dają szansę na spętanie celu i innego pobliskiego wroga. Skrępowani wrogowie mają zmniejszoną prędkość ruchu i otrzymują ciągłe obrażenia fizyczne.</string>
	<string name="skills.paladin.mastery_1.passive_1.up3.name"></string>
	<string name="skills.paladin.mastery_1.passive_1.up3.desc"></string>

	<string name="skills.paladin.mastery_2.name">Boski</string>
	<string name="skills.paladin.mastery_2.desc"></string>

	<string name="skills.paladin.mastery_2.spell_1.name">Boskie filary</string>
	<string name="skills.paladin.mastery_2.spell_1.desc">Przywołujesz słupy niebiańskiego światła, które eksplodują w twoim miejscu, zadając obrażenia fizyczne i powodując \cb0dfffŚlepotę\d.</string>
	<string name="skills.paladin.mastery_2.spell_1.up1.name">Boski Wzór</string>
	<string name="skills.paladin.mastery_2.spell_1.up1.desc">Powoduje, że dodatkowe filary uderzają kolejno w układzie przypominającym siatkę. Filary zadają o połowę mniej obrażeń przy każdym kolejnym uderzeniu.</string>
	<string name="skills.paladin.mastery_2.spell_1.up2.name">Filary gniewu</string>
	<string name="skills.paladin.mastery_2.spell_1.up2.desc">Zadaje dodatkowe obrażenia od błyskawic.</string>
	<string name="skills.paladin.mastery_2.spell_1.up3.name"></string>
	<string name="skills.paladin.mastery_2.spell_1.up3.desc"></string>

	<string name="skills.paladin.mastery_2.passive_1.name">Boska ochrona</string>
	<string name="skills.paladin.mastery_2.passive_1.desc">Otrzymanie obrażeń poniżej połowy zdrowia ma szansę zapewnić ci wzmocnienie w postaci \cb0dfffNiewrażliwości\d na krótki czas. Efekt ten ma czas odnowienia.</string>
	<string name="skills.paladin.mastery_2.passive_1.up1.name">Przywrócenie</string>
	<string name="skills.paladin.mastery_2.passive_1.up1.desc">Zyskujesz ogromny wzrost regeneracji zdrowia, gdy posiadasz to wzmocnienie.</string>
	<string name="skills.paladin.mastery_2.passive_1.up2.name">Boska riposta</string>
	<string name="skills.paladin.mastery_2.passive_1.up2.desc">Kiedy otrzymasz wzmocnienie, automatycznie rzucisz \cb0dfffBoskie Filary\d.</string>
	<string name="skills.paladin.mastery_2.passive_1.up3.name"></string>
	<string name="skills.paladin.mastery_2.passive_1.up3.desc"></string>

	<string name="skills.paladin.mastery_3.name">Radiance</string>
	<string name="skills.paladin.mastery_3.desc"></string>

	<string name="skills.paladin.mastery_3.spell_1.name">Immolacja</string>
	<string name="skills.paladin.mastery_3.spell_1.desc">Ładujesz potężną eksplozję, która zostaje wyzwolona po zwolnieniu zaklęcia, zadając obrażenia od ognia wszystkim wrogom wokół ciebie.</string>
	<string name="skills.paladin.mastery_3.spell_1.up1.name">Podsycanie płomienia</string>
	<string name="skills.paladin.mastery_3.spell_1.up1.desc">Zyskujesz dodatkowe stosy \cb0dfffInner Flames\d podczas rzucania \cb0dfffImmolation\d.</string>
	<string name="skills.paladin.mastery_3.spell_1.up2.name">Blazing Lance</string>
	<string name="skills.paladin.mastery_3.spell_1.up2.desc">Wystrzeliwujesz teraz płonącą lancę, która zadaje dodatkowe obrażenia od ognia wszystkim wrogom na swojej drodze.</string>
	<string name="skills.paladin.mastery_3.spell_1.up3.name"></string>
	<string name="skills.paladin.mastery_3.spell_1.up3.desc"></string>

	<string name="skills.paladin.mastery_3.passive_1.name">Wewnętrzne płomienie</string>
	<string name="skills.paladin.mastery_3.passive_1.desc">Rzucanie zaklęć generuje stosy \cb0dfffInner Flames\d. Kiedy masz stosy \cb0dfffInner Flames\d, zyskujesz aurę zadającą obrażenia od ognia pobliskim wrogom.</string>
	<string name="skills.paladin.mastery_3.passive_1.up1.name">Płomienie oddania</string>
	<string name="skills.paladin.mastery_3.passive_1.up1.desc">Każdy stos \cb0dfffInner Flames\d dodaje obrażenia od ognia do broni.</string>
	<string name="skills.paladin.mastery_3.passive_1.up2.name">Nosiciel płomieni</string>
	<string name="skills.paladin.mastery_3.passive_1.up2.desc">Każdy stos \cb0dfffInner Flames\d zwiększa odporność na ogień i penetrację ognia.</string>
	<string name="skills.paladin.mastery_3.passive_1.up3.name"></string>
	<string name="skills.paladin.mastery_3.passive_1.up3.desc"></string>



	<!-- Ranger -->

	<string name="skills.ranger.spell_1.name">Marked Shot</string>
	<string name="skills.ranger.spell_1.desc">Wystrzeliwujesz pocisk, który zadaje obrażenia fizyczne i nakłada \cb0dfffMarked\d na wszystkich wrogów na wybranym obszarze. Zadajesz większe obrażenia wrogom, na których nałożono \cb0dfffMarked\d.</string>
	<string name="skills.ranger.spell_1.up1.name">Gorący pościg</string>
	<string name="skills.ranger.spell_1.up1.desc">Zyskujesz krótkie wzmocnienie zwiększające szybkość ruchu przy trafianiu oznaczonych celów.</string>
	<string name="skills.ranger.spell_1.up2.name">Thrill of the Hunt</string>
	<string name="skills.ranger.spell_1.up2.desc">Zabijanie wrogów zadanych przez \cb0dfffMarked\d może przywrócić ci część zdrowia i many.</string>
	<string name="skills.ranger.spell_1.up3.name">Big Game Hunter</string>
	<string name="skills.ranger.spell_1.up3.desc">Zadajesz dodatkowe obrażenia \cb0dfffBossom\d i \cb0dfffElites\d zadane przez \cb0dfffMarked\d.</string>

	<string name="skills.ranger.spell_2.name">Pułapka</string>
	<string name="skills.ranger.spell_2.desc">Rzuca pułapkę, która uruchamia się po krótkim czasie. Po uruchomieniu pułapki zadają obrażenia fizyczne na niewielkim obszarze i powodują \cb0dfffCripple\d.</string>
	<string name="skills.ranger.spell_2.up1.name">Wdrażanie wielokrotne</string>
	<string name="skills.ranger.spell_2.up1.desc">Z każdym rzutem rozmieszczasz dodatkowe pułapki.</string>
	<string name="skills.ranger.spell_2.up2.name">Ząbkowane pułapki</string>
	<string name="skills.ranger.spell_2.up2.desc">Pułapki zadają teraz stosy \cb0dfffBleeding\d.</string>
	<string name="skills.ranger.spell_2.up3.name">Pułapki mrozowe</string>
	<string name="skills.ranger.spell_2.up3.desc">Pułapki zadają dodatkowe obrażenia od lodu.</string>

	<string name="skills.ranger.passive_1.name">Rytm</string>
	<string name="skills.ranger.passive_1.desc">Zyskujesz szansę na trafienie krytyczne, a zdobywanie trafień krytycznych zapewnia ci stosy \cb0dfffRhythm\d. Każda kumulacja \cb0dfffRhythm\d zwiększa szansę na trafienie krytyczne i obrażenia od broni.</string>
	<string name="skills.ranger.passive_1.up1.name">In the Flow</string>
	<string name="skills.ranger.passive_1.up1.desc">Każdy stos \cb0dfffRhythm\d zwiększa szybkość broni.</string>
	<string name="skills.ranger.passive_1.up2.name">Wydajność</string>
	<string name="skills.ranger.passive_1.up2.desc">Każdy stos \cb0dfffRhythm\d zmniejsza twój koszt many.</string>
	<string name="skills.ranger.passive_1.up3.name">Momentum</string>
	<string name="skills.ranger.passive_1.up3.desc">Podczas biegu zyskujesz stos \cb0dfffRhythm\d.</string>

	<string name="skills.ranger.passive_2.name">Survivalista</string>
	<string name="skills.ranger.passive_2.desc">Za każdym razem, gdy zostaniesz trafiony, zyskujesz wzmocnienie, które zmniejsza otrzymywane obrażenia na krótki czas. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.ranger.passive_2.up1.name">Manewr unikowy</string>
	<string name="skills.ranger.passive_2.up1.desc">Zapewnia również unik na czas trwania wzmocnienia.</string>
	<string name="skills.ranger.passive_2.up2.name">Manewr taktyczny</string>
	<string name="skills.ranger.passive_2.up2.desc">Otrzymując obrażenia, zyskujesz Dash Charge. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.ranger.passive_2.up3.name">Szybkie odzyskiwanie</string>
	<string name="skills.ranger.passive_2.up3.desc">Zwiększa regenerację zdrowia na czas trwania wzmocnienia.</string>


	<string name="skills.ranger.mastery_1.name">Strzelec wyborowy</string>
	<string name="skills.ranger.mastery_1.desc"></string>

	<string name="skills.ranger.mastery_1.spell_1.name">Deszcz strzał</string>
	<string name="skills.ranger.mastery_1.spell_1.desc">Wystrzeliwujesz w powietrze serię strzał, które uderzają w ziemię, zadając obrażenia fizyczne.</string>
	<string name="skills.ranger.mastery_1.spell_1.up1.name">Burza Strzałów</string>
	<string name="skills.ranger.mastery_1.spell_1.up1.desc">Zwiększa obszar uderzenia.</string>
	<string name="skills.ranger.mastery_1.spell_1.up2.name">Thundercaller</string>
	<string name="skills.ranger.mastery_1.spell_1.up2.desc">Po strzałach następuje sekwencja błyskawic, zadających obrażenia od błyskawic.</string>
	<string name="skills.ranger.mastery_1.spell_1.up3.name"></string>
	<string name="skills.ranger.mastery_1.spell_1.up3.desc"></string>

	<string name="skills.ranger.mastery_1.passive_1.name">Łucznictwo</string>
	<string name="skills.ranger.mastery_1.passive_1.desc">Zmniejszona zostaje kara do prędkości ruchu za korzystanie z umiejętności \cffdfb0Bow\d, a umiejętności \cffdfb0Bow\d zadają większe obrażenia.</string>
	<string name="skills.ranger.mastery_1.passive_1.up1.name">Szybki strzał</string>
	<string name="skills.ranger.mastery_1.passive_1.up1.desc">Umiejętności \cffdfb0Bow\d działają szybciej.</string>
	<string name="skills.ranger.mastery_1.passive_1.up2.name">Strzały mrozu</string>
	<string name="skills.ranger.mastery_1.passive_1.up2.desc">Twoje umiejętności \cffdfb0Bow\d zyskują szansę na wywołanie eksplozji mrozu przy trafieniu, zadając obrażenia od lodu. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.ranger.mastery_1.passive_1.up3.name"></string>
	<string name="skills.ranger.mastery_1.passive_1.up3.desc"></string>

	<string name="skills.ranger.mastery_2.name">Beastmaster</string>
	<string name="skills.ranger.mastery_2.desc"></string>

	<string name="skills.ranger.mastery_2.spell_1.name">Spirit Wolves</string>
	<string name="skills.ranger.mastery_2.spell_1.desc">Uwalniasz duchowe wilki, które szukają pobliskich wrogów, zadając obrażenia fizyczne i od lodu.</string>
	<string name="skills.ranger.mastery_2.spell_1.up1.name">Nieustępliwi Łowcy</string>
	<string name="skills.ranger.mastery_2.spell_1.up1.desc">Wilki duchowe zyskują szansę na znalezienie nowego celu po trafieniu.</string>
	<string name="skills.ranger.mastery_2.spell_1.up2.name">Niespokojne duchy</string>
	<string name="skills.ranger.mastery_2.spell_1.up2.desc">Zyskujesz szansę na uwolnienie duchowego wilka podczas zabijania wrogów. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.ranger.mastery_2.spell_1.up3.name"></string>
	<string name="skills.ranger.mastery_2.spell_1.up3.desc"></string>

	<string name="skills.ranger.mastery_2.passive_1.name">Wolf Companion</string>
	<string name="skills.ranger.mastery_2.passive_1.desc">Pasywnie przywołujesz wilczego towarzysza, który atakuje pobliskich wrogów, zadając obrażenia fizyczne.</string>
	<string name="skills.ranger.mastery_2.passive_1.up1.name">Wolf Pack</string>
