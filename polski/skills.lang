	<string name="skills.rogue.mastery_2.desc"></string>

	<string name="skills.rogue.mastery_2.spell_1.name">Toksyczna chmura</string>
	<string name="skills.rogue.mastery_2.spell_1.desc">Tworzysz dużą toksyczną chmurę, która utrzymuje się przez pewien czas. Chmura blokuje pole widzenia przeciwnika i zadaje obrażenia od trucizny wszystkim wrogom znajdującym się wewnątrz chmury.</string>
	<string name="skills.rogue.mastery_2.spell_1.up1.name">Rozwiewająca się chmura</string>
	<string name="skills.rogue.mastery_2.spell_1.up1.desc">Ty i twoi sojusznicy zyskujecie unik podczas przebywania w chmurze.</string>
	<string name="skills.rogue.mastery_2.spell_1.up2.name">Korozja</string>
	<string name="skills.rogue.mastery_2.spell_1.up2.desc">Wrogowie wchodzący w chmurę otrzymują obrażenia od \cb0dfffArmor Break\d.</string>
	<string name="skills.rogue.mastery_2.spell_1.up3.name"></string>
	<string name="skills.rogue.mastery_2.spell_1.up3.desc"></string>

	<string name="skills.rogue.mastery_2.passive_1.name">Opanowanie trucizny</string>
	<string name="skills.rogue.mastery_2.passive_1.desc">Zadajesz zwiększone obrażenia od trucizny, a twoje umiejętności \cffdfb0Poison\d zyskują szansę na zadanie \cb0dfffVulnerability\d.</string>
	<string name="skills.rogue.mastery_2.passive_1.up1.name">Broń powlekana</string>
	<string name="skills.rogue.mastery_2.passive_1.up1.desc">Dodaje obrażenia od trucizny do broni na poziom postaci.</string>
	<string name="skills.rogue.mastery_2.passive_1.up2.name">Śmiercionośne toksyny</string>
	<string name="skills.rogue.mastery_2.passive_1.up2.desc">Zwiększa penetrację trucizny i odporność na truciznę.</string>
	<string name="skills.rogue.mastery_2.passive_1.up3.name"></string>
	<string name="skills.rogue.mastery_2.passive_1.up3.desc"></string>

