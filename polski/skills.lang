	<!-- Sorcerer -->

	<string name="skills.sorcerer.spell_1.name">Wyładowanie</string>
	<string name="skills.sorcerer.spell_1.desc">Uwalniasz kilka łuków błyskawic w stożku, zadając obrażenia od błyskawic. Każdy wróg może zostać trafiony tylko jednym łukiem.</string>
	<string name="skills.sorcerer.spell_1.up1.name">Chain Lightning</string>
	<string name="skills.sorcerer.spell_1.up1.desc">Każdy łuk błyskawicy może łączyć się z dodatkowymi celami.</string>
	<string name="skills.sorcerer.spell_1.up2.name">Przedłużone rozładowanie</string>
	<string name="skills.sorcerer.spell_1.up2.desc">Zwiększa zasięg zaklęcia.</string>
	<string name="skills.sorcerer.spell_1.up3.name">Palące promienie</string>
	<string name="skills.sorcerer.spell_1.up3.desc">Zmieniono typ obrażeń na ogień, a promienie przebijają teraz cele.</string>
	<string name="skills.sorcerer.spell_1.up4.name">Promienie mrozu</string>
	<string name="skills.sorcerer.spell_1.up4.desc">Uwalnia mniej promieni w ciaśniejszym stożku, ale wrogowie mogą teraz zostać trafieni kilkoma promieniami. Zmieniono typ obrażeń na lodowe.</string>

	<string name="skills.sorcerer.spell_2.name">Kula żywiołów</string>
	<string name="skills.sorcerer.spell_2.desc">Wyczarowujesz wolno poruszającą się kulę, która nieustannie zadaje obrażenia od wszystkich żywiołów pobliskim wrogom.</string>
	<string name="skills.sorcerer.spell_2.up1.name">Adaptive Orb</string>
	<string name="skills.sorcerer.spell_2.up1.desc">Jeśli podczas rzucania zaklęcia masz \cb0dfffElemental Affinity\d, kula zada obrażenia tylko od odpowiedniego żywiołu.</string>
	<string name="skills.sorcerer.spell_2.up2.name">Stabilna kula</string>
	<string name="skills.sorcerer.spell_2.up2.desc">Wydłuża żywotność kuli.</string>
	<string name="skills.sorcerer.spell_2.up3.name">Elemental Torrent</string>
	<string name="skills.sorcerer.spell_2.up3.desc">Kula zaczyna teraz wyrzucać mniejsze pociski we wszystkich kierunkach. Pociski te zadają mniejsze obrażenia.</string>

	<string name="skills.sorcerer.passive_1.name">Powinowactwo żywiołów</string>
	<string name="skills.sorcerer.passive_1.desc">Podczas rzucania \cffdfb0Zaklęcia\d zyskujesz \cb0dfffPrzyległość Żywiołów\d tego samego żywiołu co rzucone zaklęcie. Zadajesz więcej obrażeń od odpowiedniego żywiołu, dopóki masz \cb0dfffElemental Affinity\d. W danym momencie możesz mieć aktywny tylko jeden rodzaj żywiołu.</string>
	<string name="skills.sorcerer.passive_1.up1.name">Oddział żywiołów</string>
	<string name="skills.sorcerer.passive_1.up1.desc">Zapewnia dodatkową odporność na ogień, lód i błyskawice na poziom postaci.</string>
	<string name="skills.sorcerer.passive_1.up2.name">Penetracja żywiołów</string>
	<string name="skills.sorcerer.passive_1.up2.desc">Zapewnia przenikanie ognia, lodu i błyskawic.</string>
	<string name="skills.sorcerer.passive_1.up3.name">Elemental Equilibrium</string>
	<string name="skills.sorcerer.passive_1.up3.desc">Dopóki posiadasz \cb0dfffElemental Affinity\d, premia do obrażeń jest podwojona dla innych żywiołów.</string>

	<string name="skills.sorcerer.passive_2.name">Wrodzona moc</string>
	<string name="skills.sorcerer.passive_2.desc">Przyznaje szansę na trafienie krytyczne zaklęcia.</string>
	<string name="skills.sorcerer.passive_2.up1.name">Niekontrolowana moc</string>
	<string name="skills.sorcerer.passive_2.up1.desc">Zadajesz zwiększone obrażenia krytyczne od zaklęć.</string>
	<string name="skills.sorcerer.passive_2.up2.name">Wzrost mocy</string>
	<string name="skills.sorcerer.passive_2.up2.desc">Otrzymujesz premię zwiększającą szybkość broni, rzucania zaklęć i poruszania się po trafieniu krytycznym zaklęcia.</string>
	<string name="skills.sorcerer.passive_2.up3.name">Przypływ Many</string>
	<string name="skills.sorcerer.passive_2.up3.desc">Przywracasz część swojej many, gdy zdobędziesz Krytyczne Zaklęcie.</string>


	<string name="skills.sorcerer.mastery_1.name">Ogień</string>
	<string name="skills.sorcerer.mastery_1.desc"></string>

	<string name="skills.sorcerer.mastery_1.spell_1.name">Ściana płomieni</string>
	<string name="skills.sorcerer.mastery_1.spell_1.desc">Tworzysz przed sobą płonącą ścianę płomieni, która zadaje obrażenia od ognia wszystkim wrogom znajdującym się w jej pobliżu.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up1.name">Flamewalker</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up1.desc">Ty i twoi sojusznicy wkraczający na ścianę zostają rozpaleni tymczasową aurą, która zadaje obrażenia od ognia pobliskim wrogom.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up2.name">Krok płomienia</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up2.desc">Odradza się pojedyncza instancja ściany podczas skoku.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up3.name">Umieszczenie na ścianie</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up3.desc">Teraz można określić położenie ściany.</string>

	<string name="skills.sorcerer.mastery_1.passive_1.name">Mistrzostwo ognia</string>
	<string name="skills.sorcerer.mastery_1.passive_1.desc">Zadajesz zwiększone obrażenia od ognia, a twoje umiejętności \cffdfb0Fire\d zyskują szansę na zadanie \cb0dfffWeakness\d.</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up1.name">Lotne płomienie</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up1.desc">Twoje umiejętności \cffdfb0Fire\d zyskują szansę na wywołanie eksplozji zadającej dodatkowe obrażenia od ognia. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up2.name">Uzdrawiające płomienie</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up2.desc">Przywracasz część swojego zdrowia, gdy zdobędziesz Krytyczne Zaklęcie.</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up3.name"></string>
	<string name="skills.sorcerer.mastery_1.passive_1.up3.desc"></string>

	<string name="skills.sorcerer.mastery_2.name">Lód</string>
	<string name="skills.sorcerer.mastery_2.desc"></string>

	<string name="skills.sorcerer.mastery_2.spell_1.name">Frost Nova</string>
	<string name="skills.sorcerer.mastery_2.spell_1.desc">Uwalniasz wiele mroźnych odłamków we wszystkich kierunkach, zadając obrażenia od lodu.</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up1.name">Poszukiwacze Mrozu</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up1.desc">Pociski zyskują szansę na znalezienie nowych celów po trafieniu.</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up2.name">Drzazgi mrozu</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up2.desc">Pociski zyskują szansę na wyzwolenie nowej, mniejszej nova pocisków podczas zabijania wrogów.</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up3.name"></string>
	<string name="skills.sorcerer.mastery_2.spell_1.up3.desc"></string>

	<string name="skills.sorcerer.mastery_2.passive_1.name">Mistrzostwo Lodu</string>
	<string name="skills.sorcerer.mastery_2.passive_1.desc">Zadajesz zwiększone obrażenia od lodu, a twoje umiejętności \cffdfb0Ice\d zyskują szansę na zadanie \cb0dfffFreeze\d.</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up1.name">Shatter</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up1.desc">Zabijanie wrogów umiejętnościami \cffdfb0Ice\d ma szansę wywołać eksplozję zadającą obrażenia od lodu.</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up2.name">Blok lodowy</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up2.desc">Zyskujesz szansę na zablokowanie obrażeń fizycznych i od lodu.</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up3.name"></string>
	<string name="skills.sorcerer.mastery_2.passive_1.up3.desc"></string>

	<string name="skills.sorcerer.mastery_3.name">Błyskawica</string>
	<string name="skills.sorcerer.mastery_3.desc"></string>

	<string name="skills.sorcerer.mastery_3.spell_1.name">Lightning Ball</string>
	<string name="skills.sorcerer.mastery_3.spell_1.desc">Ładujesz kulę błyskawic, która po rzuceniu zostaje uwolniona i zadaje obrażenia od błyskawic wszystkim wrogom na swojej drodze. Obrażenia zadawane przez pocisk zależą od czasu ładowania zaklęcia.</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up1.name">Energized Ball</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up1.desc">Każde trafienie przeciwnika wydłuża żywotność pocisku.</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up2.name">Pole statyczne</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up2.desc">Pozostawiasz po sobie pole elektryczne, które utrzymuje się przez pewien czas, zadając dodatkowe obrażenia od błyskawic.</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up3.name"></string>
	<string name="skills.sorcerer.mastery_3.spell_1.up3.desc"></string>
	
	<string name="skills.sorcerer.mastery_3.passive_1.name">Mistrzostwo błyskawic</string>
	<string name="skills.sorcerer.mastery_3.passive_1.desc">Zadajesz zwiększone obrażenia od błyskawic, a twoje umiejętności \cb0dffb0Lightning\d zyskują szansę na zadanie \cb0dfffStun\d.</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up1.name">Arc Lightning</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up1.desc">Twoje umiejętności \cffdfb0Lightning\d zyskują szansę na wyzwolenie łuku elektrycznego, który łączy wrogów, zadając obrażenia od błyskawic.</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up2.name">Pole zerowe</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up2.desc">Zyskujesz szansę na zablokowanie większości pocisków. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up3.name"></string>
	<string name="skills.sorcerer.mastery_3.passive_1.up3.desc"></string>











	<!-- Warlock -->

	<string name="skills.warlock.spell_1.name">Ogon Demona</string>
	<string name="skills.warlock.spell_1.desc">Wyczarowuje diabelski bicz, który leci do przodu, zadając obrażenia fizyczne i \cb0dfffSlow\d.</string>
	<string name="skills.warlock.spell_1.up1.name">Crackling Lash</string>
	<string name="skills.warlock.spell_1.up1.desc">W pobliskich wrogów wystrzeliwane są trzaskające łuki błyskawic, które zadają obrażenia od błyskawic.</string>
	<string name="skills.warlock.spell_1.up2.name">Fiery Lash</string>
	<string name="skills.warlock.spell_1.up2.desc">Wzdłuż rzęs wybuchają płomienie, które utrzymują się przez krótki czas, zadając obrażenia od ognia.</string>
	<string name="skills.warlock.spell_1.up3.name">Silne uderzenie</string>
	<string name="skills.warlock.spell_1.up3.desc">Wrogowie trafieni końcem bicza otrzymują teraz również \cb0dfffStun\d.</string>

	<string name="skills.warlock.spell_2.name">Mroczny rytuał</string>
	<string name="skills.warlock.spell_2.desc">Rozpoczynasz rytuał, który kończy się po kilku sekundach. Wrogowie otrzymują obrażenia fizyczne podczas przebywania w obszarze i jeszcze większe obrażenia po zakończeniu rytuału. Rytuał zostanie przerwany, jeśli opuścisz obszar przed jego zakończeniem.</string>
	<string name="skills.warlock.spell_2.up1.name">Rytuał Krwi</string>
	<string name="skills.warlock.spell_2.up1.desc">Wrogowie znajdujący się wewnątrz obszaru mogą teraz otrzymać stosy Krwawienia.</string>
	<string name="skills.warlock.spell_2.up2.name">Konflikt</string>
	<string name="skills.warlock.spell_2.up2.desc">Typ obrażeń zostaje zmieniony na ogień, a rytuał zadaje jeszcze więcej obrażeń po zakończeniu.</string>
	<string name="skills.warlock.spell_2.up3.name">Rytuał pijawki</string>
	<string name="skills.warlock.spell_2.up3.desc">Ty i twoi sojusznicy zyskujecie \cb0dfffLife Leech\d podczas przebywania na tym obszarze.</string>

	<string name="skills.warlock.passive_1.name">Drain Essence</string>
	<string name="skills.warlock.passive_1.desc">Zdobywasz stosy \cb0dfffEssence\d, gdy trafiasz wrogów umiejętnościami \cb0dfb0Melee\d. Każda kumulacja zwiększa twoje zdrowie i manę. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.warlock.passive_1.up1.name">Life Siphon</string>
	<string name="skills.warlock.passive_1.up1.desc">Kiedy zdobywasz stosy \cb0dfffEssence\d, przywracasz procent swojego zdrowia.</string>
	<string name="skills.warlock.passive_1.up2.name">Syfon energetyczny</string>
	<string name="skills.warlock.passive_1.up2.desc">Kiedy zdobywasz stosy \cb0dfffEssence\d, przywracasz procent swojej many.</string>
	<string name="skills.warlock.passive_1.up3.name">Drain Blood</string>
	<string name="skills.warlock.passive_1.up3.desc">Teraz zadajesz również stosy Krwawienia celom, od których otrzymujesz Esencję.</string>
	<string name="skills.warlock.passive_1.up4.name">Essence Harvester</string>
	<string name="skills.warlock.passive_1.up4.desc">Zabijając wrogów, zyskujesz szansę na zdobycie stosów \cb0dfffEssence\d.</string>

	<string name="skills.warlock.passive_2.name">Aura atrofii</string>
	<string name="skills.warlock.passive_2.desc">Tworzysz wokół siebie aurę, która sprawia, że wrogowie zadają mniej obrażeń.</string>
	<string name="skills.warlock.passive_2.up1.name">Agonia</string>
	<string name="skills.warlock.passive_2.up1.desc">Wrogowie znajdujący się w twojej aurze również otrzymują więcej obrażeń.</string>
	<string name="skills.warlock.passive_2.up2.name">Rozprzestrzeniająca się korupcja</string>
	<string name="skills.warlock.passive_2.up2.desc">\Trafienia z broni do walki wręcz zadają dodatkowe obrażenia od trucizny innemu pobliskiemu wrogowi.</string>
	<string name="skills.warlock.passive_2.up3.name">Spontaniczne spalanie</string>
	<string name="skills.warlock.passive_2.up3.desc">Rzucenie \cffdfb0Zaklęcia\d zadaje obrażenia od ognia wszystkim wrogom znajdującym się pod wpływem twojej aury.</string>


	<string name="skills.warlock.mastery_1.name">Przywołanie</string>
	<string name="skills.warlock.mastery_1.desc"></string>

	<string name="skills.warlock.mastery_1.spell_1.name">Przywołanie diabłów</string>
	<string name="skills.warlock.mastery_1.spell_1.desc">Użyj zaklęcia, aby przywołać diabły, zużywając stosy \cb0dfffEssence\d. Diabły atakują pobliskich wrogów, zadając obrażenia fizyczne.</string>
	<string name="skills.warlock.mastery_1.spell_1.up1.name">Piekielni wrogowie</string>
	<string name="skills.warlock.mastery_1.spell_1.up1.desc">Twoje diabły zadają dodatkowe obrażenia od ognia i zyskują zdolność wyrzucania ognistych kul w swoje cele.</string>
	<string name="skills.warlock.mastery_1.spell_1.up2.name">Wybuchowa śmierć</string>
	<string name="skills.warlock.mastery_1.spell_1.up2.desc">Twoje diabły eksplodują po śmierci, zadając obrażenia fizyczne na niewielkim obszarze.</string>
	<string name="skills.warlock.mastery_1.spell_1.up3.name"></string>
	<string name="skills.warlock.mastery_1.spell_1.up3.desc"></string>

	<string name="skills.warlock.mastery_1.passive_1.name">Sługa gargulca</string>
	<string name="skills.warlock.mastery_1.passive_1.desc">Pasywnie przywołuje gargulca, aby walczył u twojego boku. Gargulec atakuje wrogów pazurami i trójzębem, zadając obrażenia fizyczne.</string>
	<string name="skills.warlock.mastery_1.passive_1.up1.name">Płonące uderzenia</string>
	<string name="skills.warlock.mastery_1.passive_1.up1.desc">Twój gargulec zadaje dodatkowe obrażenia od ognia swoimi pazurami.</string>
	<string name="skills.warlock.mastery_1.passive_1.up2.name">Lightning Trident</string>
	<string name="skills.warlock.mastery_1.passive_1.up2.desc">Trójząb gargulca zadaje teraz wrogom dodatkowe obrażenia od błyskawic.</string>
	<string name="skills.warlock.mastery_1.passive_1.up3.name"></string>
	<string name="skills.warlock.mastery_1.passive_1.up3.desc"></string>

	<string name="skills.warlock.mastery_2.name">Dusze</string>
	<string name="skills.warlock.mastery_2.desc"></string>

	<string name="skills.warlock.mastery_2.spell_1.name">Wir duszy</string>
	<string name="skills.warlock.mastery_2.spell_1.desc">Tworzysz wokół siebie wir energii duszy, który zadaje obrażenia fizyczne i od trucizny wszystkim wrogom znajdującym się w pobliżu.</string>
	<string name="skills.warlock.mastery_2.spell_1.up1.name">Lightning Vortex</string>
	<string name="skills.warlock.mastery_2.spell_1.up1.desc">Wir nieustannie wyzwala teraz błyskawice, które losowo uderzają w obszar wokół ciebie, zadając dodatkowe obrażenia od błyskawic.</string>
	<string name="skills.warlock.mastery_2.spell_1.up2.name">Vortex Shroud</string>
	<string name="skills.warlock.mastery_2.spell_1.up2.desc">Na czas trwania zaklęcia zyskujesz umiejętność Unik.</string>
	<string name="skills.warlock.mastery_2.spell_1.up3.name"></string>
	<string name="skills.warlock.mastery_2.spell_1.up3.desc"></string>

	<string name="skills.warlock.mastery_2.passive_1.name">Soul Shaper</string>
	<string name="skills.warlock.mastery_2.passive_1.desc">Dopóki posiadasz stosy \cb0dfffEssence\d, twoje ataki bronią w ręce głównej uwalniają pocisk duszy w kierunku losowego pobliskiego wroga, zadając obrażenia od ręki głównej.</string>
	<string name="skills.warlock.mastery_2.passive_1.up1.name">Soul Cleave</string>
	<string name="skills.warlock.mastery_2.passive_1.up1.desc">Pociski mają szansę eksplodować, zadając dodatkowe obrażenia. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.warlock.mastery_2.passive_1.up2.name">Nieustępliwe dusze</string>
	<string name="skills.warlock.mastery_2.passive_1.up2.desc">Pociski przeskakują na dodatkowe cele po trafieniu.</string>
	<string name="skills.warlock.mastery_2.passive_1.up3.name"></string>
	<string name="skills.warlock.mastery_2.passive_1.up3.desc"></string>

	<string name="skills.warlock.mastery_3.name">Chaos</string>
	<string name="skills.warlock.mastery_3.desc"></string>

	<string name="skills.warlock.mastery_3.spell_1.name">Ostrze Chaosu</string>
	<string name="skills.warlock.mastery_3.spell_1.desc">Wyczarowujesz magiczne ostrze, które tnie obszar przed tobą, zadając obrażenia fizyczne i od błyskawic.</string>
	<string name="skills.warlock.mastery_3.spell_1.up1.name">Łuki ostrzy</string>
	<string name="skills.warlock.mastery_3.spell_1.up1.desc">Zaklęcie wystrzeliwuje teraz także kilka pocisków w łuku przed tobą.</string>
	<string name="skills.warlock.mastery_3.spell_1.up2.name">Zerwanie</string>
	<string name="skills.warlock.mastery_3.spell_1.up2.desc">Cięcie ostrzem zadaje teraz stosy \cb0dfffKrwawienia\d.</string>
	<string name="skills.warlock.mastery_3.spell_1.up3.name"></string>
	<string name="skills.warlock.mastery_3.spell_1.up3.desc"></string>
	
	<string name="skills.warlock.mastery_3.passive_1.name">Szczeliny Chaosu</string>
	<string name="skills.warlock.mastery_3.passive_1.desc">Rzucenie \cffdfb0Zaklęcia\d powoduje powstanie szczeliny, która zadaje obrażenia fizyczne i od błyskawic pobliskim wrogom. Szczeliny powstałe blisko siebie tworzą między sobą strumienie energii, które zadają dodatkowe obrażenia.</string>
	<string name="skills.warlock.mastery_3.passive_1.up1.name">Żniwiarz Chaosu</string>
	<string name="skills.warlock.mastery_3.passive_1.up1.desc">Zabicie wroga może spowodować powstanie szczeliny. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.warlock.mastery_3.passive_1.up2.name">Rift Blade</string>
	<string name="skills.warlock.mastery_3.passive_1.up2.desc">Trafienie Ostrzem Chaosu daje szansę na stworzenie szczeliny. Efekt ten ma krótki czas odnowienia.</string>
	<string name="skills.warlock.mastery_3.passive_1.up3.name"></string>
	<string name="skills.warlock.mastery_3.passive_1.up3.desc"></string>








	<!-- Warrior -->


	<string name="skills.warrior.spell_1.name">Wirujące osie</string>
	<string name="skills.warrior.spell_1.desc">Rzucasz dwoma wirującymi toporami, które zadają obrażenia fizyczne wszystkim wrogom na swojej drodze.</string>
	<string name="skills.warrior.spell_1.up1.name">Osie obrotowe</string>
	<string name="skills.warrior.spell_1.up1.desc">Oś powróci do użytkownika po pewnym czasie.</string>
	<string name="skills.warrior.spell_1.up2.name">Płonące topory</string>
	<string name="skills.warrior.spell_1.up2.desc">Topory zadają dodatkowe obrażenia od ognia.</string>
	<string name="skills.warrior.spell_1.up3.name">Topory do uboju</string>
	<string name="skills.warrior.spell_1.up3.desc">Topory zyskują szansę na natychmiastowe zabicie wroga na \cb0dfffNiskim poziomie życia\d. Nie działa na bossów.</string>

	<string name="skills.warrior.spell_2.name">Okrzyk wojenny</string>
	<string name="skills.warrior.spell_2.desc">Ty i wszyscy sojusznicy w okolicy zyskujecie wzmocnienie, które na pewien czas zwiększa szybkość broni i ruchu.</string>
	<string name="skills.warrior.spell_2.up1.name">Zastraszający krzyk</string>
	<string name="skills.warrior.spell_2.up1.desc">Wrogowie wokół ciebie otrzymują \cb0dfffSłabość\d podczas używania Okrzyku Wojennego.</string>
	<string name="skills.warrior.spell_2.up2.name">Drugi wiatr</string>
	<string name="skills.warrior.spell_2.up2.desc">Podczas używania Okrzyku Wojennego odzyskujesz część zdrowia.</string>
	<string name="skills.warrior.spell_2.up3.name">Okrzyk bojowy</string>
	<string name="skills.warrior.spell_2.up3.desc">Podczas używania Okrzyku Wojennego zyskujesz stosy \cb0dfffKilling Spree\d za każdego wroga wokół ciebie.</string>

	<string name="skills.warrior.passive_1.name">Konstytucja Wojownika</string>
	<string name="skills.warrior.passive_1.desc">Zwiększa twoje zdrowie i odporność na trucizny.</string>
	<string name="skills.warrior.passive_1.up1.name">Trening bojowy</string>
	<string name="skills.warrior.passive_1.up1.desc">Umiejętności \cffdfb0Melee\d zadają większe obrażenia.</string>
	<string name="skills.warrior.passive_1.up2.name">Wytrzymałość</string>
	<string name="skills.warrior.passive_1.up2.desc">Otrzymujesz mniej obrażeń.</string>
	<string name="skills.warrior.passive_1.up3.name">Potężna siła</string>
	<string name="skills.warrior.passive_1.up3.desc">Zwiększa penetrację pancerza.</string>

	<string name="skills.warrior.passive_2.name">Szał zabijania</string>
	<string name="skills.warrior.passive_2.desc">Zabijając wrogów, zdobywasz stosy \cb0dfffKilling Spree\d. Każdy poziom zwiększa zadawane obrażenia. Tracisz wszystkie stosy naraz, jeśli skończy się ich czas trwania.</string>
	<string name="skills.warrior.passive_2.up1.name">Brutalność</string>
	<string name="skills.warrior.passive_2.up1.desc">Każdy stos zapewnia teraz również szansę na trafienie krytyczne broni i zaklęć.</string>
	<string name="skills.warrior.passive_2.up2.name">Fizyczny zabójca</string>
	<string name="skills.warrior.passive_2.up2.desc">Każdy stos zwiększa regenerację zdrowia.</string>
	<string name="skills.warrior.passive_2.up3.name">Dauntless</string>
	<string name="skills.warrior.passive_2.up3.desc">Trafienia \cbdfb0Melee\d przeciwko \cb0dfffBossom\d i \cb0dfffElites\d zyskują szansę na dodanie stosów \cb0dfffKilling Spree\d.</string>

	
	
	
	<string name="skills.warrior.mastery_1.name">Berserker</string>
	<string name="skills.warrior.mastery_1.desc"></string>

	<string name="skills.warrior.mastery_1.spell_1.name">Skok</string>
	<string name="skills.warrior.mastery_1.spell_1.desc">Skaczesz w kierunku celu, lądując z impetem, zadając obrażenia fizyczne i ogłuszając znajdujących się w pobliżu wrogów.</string>
	<string name="skills.warrior.mastery_1.spell_1.up1.name">Wpływ drżenia</string>
	<string name="skills.warrior.mastery_1.spell_1.up1.desc">Po wylądowaniu wywołujesz serię wstrząsów, które wybuchają po krótkim czasie, zadając dodatkowe obrażenia fizyczne.</string>
	<string name="skills.warrior.mastery_1.spell_1.up2.name">Ognisty skok</string>
	<string name="skills.warrior.mastery_1.spell_1.up2.desc">Podpalasz ziemię w miejscu, z którego skaczesz i lądujesz, zadając obrażenia od ognia przez pewien czas.</string>
	<string name="skills.warrior.mastery_1.spell_1.up3.name"></string>
	<string name="skills.warrior.mastery_1.spell_1.up3.desc"></string>

	<string name="skills.warrior.mastery_1.passive_1.name">Wściekłość</string>
	<string name="skills.warrior.mastery_1.passive_1.desc">Otrzymanie obrażeń zapewni ci tymczasowe wzmocnienie \cb0dfffRage\d, zwiększające szansę na trafienie krytyczne i obrażenia krytyczne umiejętności \cffdfb0Physical\d.</string>
	<string name="skills.warrior.mastery_1.passive_1.up1.name">Nieustępliwy gniew</string>
	<string name="skills.warrior.mastery_1.passive_1.up1.desc">Automatycznie zyskujesz wściekłość, gdy jesteś na \cb0dfffLow Life\d, a także otrzymujesz mniej obrażeń, gdy jesteś na \cb0dfffLow Life\d.</string>
	<string name="skills.warrior.mastery_1.passive_1.up2.name">Wściekły krzyk</string>
