	<string name="skills.ranger.mastery_2.passive_1.up2.name">Zimowy Wilk</string>
	<string name="skills.ranger.mastery_2.passive_1.up2.desc">Twoje wilki zadają dodatkowe obrażenia od lodu.</string>
