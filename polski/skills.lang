	<!-- Rogue -->

	<string name="skills.rogue.spell_1.name">Fan noży</string>
	<string name="skills.rogue.spell_1.desc">Rzuca kilkoma nożami w stożku, zadając obrażenia fizyczne.</string>
	<string name="skills.rogue.spell_1.up1.name">Rykoszet</string>
	<string name="skills.rogue.spell_1.up1.desc">Pociski odbijają się teraz od ścian i kolizji. Każde odbicie zmniejsza prędkość i obrażenia.</string>
	<string name="skills.rogue.spell_1.up2.name">Śmiertelny cel</string>
	<string name="skills.rogue.spell_1.up2.desc">Zyskujesz zwiększoną szansę na trafienie krytyczne dzięki \cb0dfff Fan of Knives\d.</string>
	<string name="skills.rogue.spell_1.up3.name">Zatrute noże</string>
	<string name="skills.rogue.spell_1.up3.desc">Zadaje dodatkowe obrażenia od trucizny.</string>
	<string name="skills.rogue.spell_1.up4.name">Skoncentrowany rzut</string>
	<string name="skills.rogue.spell_1.up4.desc">Rzucasz mniej pocisków w kierunku do przodu. Pociski zadają więcej obrażeń i mają nieco dłuższą żywotność.</string>

	<string name="skills.rogue.spell_2.name">Flurry of Blades</string>
	<string name="skills.rogue.spell_2.desc">Wykonujesz serię uderzeń, trafiając wszystkich wrogów na obszarze wokół ciebie i zadając obrażenia fizyczne.</string>
	<string name="skills.rogue.spell_2.up1.name">Uderzenia rozbrajające</string>
	<string name="skills.rogue.spell_2.up1.desc">Zyskuje szansę na zadanie \cb0dfffDisarm\d.</string>
	<string name="skills.rogue.spell_2.up2.name">Ognisty Bolas</string>
	<string name="skills.rogue.spell_2.up2.desc">Bolas jest zadawany każdemu trafionemu celowi oraz dodatkowemu wrogowi w pobliżu. Trafieni wrogowie zostają spowolnieni i przez pewien czas otrzymują obrażenia od ognia.</string>
	<string name="skills.rogue.spell_2.up3.name">Atak szokowy</string>
	<string name="skills.rogue.spell_2.up3.desc">Zadaje dodatkowe obrażenia od błyskawic</string>

	<string name="skills.rogue.passive_1.name">Slice and Dice</string>
	<string name="skills.rogue.passive_1.desc">Trafienia umiejętnością \cffdfb0Melee\d zapewniają stosy \cb0dfffSlice and Dice\d. Każda kumulacja zwiększa szybkość broni. Jeśli czas trwania kumulacji dobiegnie końca, wszystkie kumulacje zostaną utracone.</string>
	<string name="skills.rogue.passive_1.up1.name">Adrenalina</string>
	<string name="skills.rogue.passive_1.up1.desc">Każdy stos zwiększa regenerację many.</string>
	<string name="skills.rogue.passive_1.up2.name">Parowanie ostrzem</string>
	<string name="skills.rogue.passive_1.up2.desc">Każdy stos zwiększa Parowanie.</string>
	<string name="skills.rogue.passive_1.up3.name">Agile Casting</string>
	<string name="skills.rogue.passive_1.up3.desc">Każdy stos zwiększa szybkość rzucania zaklęć, a stosy zyskuje się także poprzez rzucanie zaklęć.</string>

	<string name="skills.rogue.passive_2.name">Sidestep</string>
	<string name="skills.rogue.passive_2.desc">Zapewnia unik i pojedynczy stos gwarantowanego uniku, który ładuje się co kilka sekund.</string>
	<string name="skills.rogue.passive_2.up1.name">Fleet Footed</string>
	<string name="skills.rogue.passive_2.up1.desc">Zwiększa szybkość ruchu.</string>
	<string name="skills.rogue.passive_2.up2.name">Zabójcze otwarcie</string>
	<string name="skills.rogue.passive_2.up2.desc">Zapewnia dodatkową szansę na trafienie krytyczne przez krótki czas po wykonaniu uniku.</string>
	<string name="skills.rogue.passive_2.up3.name">W ruchu</string>
	<string name="skills.rogue.passive_2.up3.desc">Dashing zyskuje szansę na ponowne naładowanie gwarantowanego uniku \cb0dfffSidestep\d.</string>


	<string name="skills.rogue.mastery_1.name">Ostrza</string>
	<string name="skills.rogue.mastery_1.desc"></string>

	<string name="skills.rogue.mastery_1.spell_1.name">Mortal Strike</string>
	<string name="skills.rogue.mastery_1.spell_1.desc">Szybko zadajesz śmiertelne obrażenia fizyczne.</string>
	<string name="skills.rogue.mastery_1.spell_1.up1.name">Phantasmal Strike</string>
	<string name="skills.rogue.mastery_1.spell_1.up1.desc">Uderzenie obejmuje teraz dodatkowych wrogów w stożku za celem. Zadaje mniej obrażeń za każdego kolejnego trafionego wroga.</string>
	<string name="skills.rogue.mastery_1.spell_1.up2.name">Crimson Strike</string>
	<string name="skills.rogue.mastery_1.spell_1.up2.desc">Dzięki \cb0dfffŚmiertelnemu Uderzeniu\d zyskujesz niewielką ilość Pijawki Życia.</string>
	<string name="skills.rogue.mastery_1.spell_1.up3.name"></string>
	<string name="skills.rogue.mastery_1.spell_1.up3.desc"></string>

	<string name="skills.rogue.mastery_1.passive_1.name">Zabójca</string>
	<string name="skills.rogue.mastery_1.passive_1.desc">Zwiększa obrażenia krytyczne broni i zmniejsza zasięg wykrywania przez wrogów.</string>
	<string name="skills.rogue.mastery_1.passive_1.up1.name">Egzekutor</string>
	<string name="skills.rogue.mastery_1.passive_1.up1.desc">Zyskujesz szansę na natychmiastowe zabicie wroga na \cffdfb0Niskim Poziomie Życia\d, gdy zdobywasz trafienia krytyczne umiejętnościami \cffdfb0Melee\d. Nie działa na bossów.</string>
	<string name="skills.rogue.mastery_1.passive_1.up2.name">Ukryte cięcia</string>
	<string name="skills.rogue.mastery_1.passive_1.up2.desc">Atak bronią do walki wręcz zadaje obrażenia od broni głównej losowemu wrogowi w pobliżu.</string>
	<string name="skills.rogue.mastery_1.passive_1.up3.name"></string>
	<string name="skills.rogue.mastery_1.passive_1.up3.desc"></string>

