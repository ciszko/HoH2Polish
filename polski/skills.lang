	<string name="skills.attacks.mainhand.mace.name">Mace Bash</string>
	<string name="skills.attacks.mainhand.mace.desc">Potężny zamach, który zadaje 100% obrażeń od broni i ma 15% szans na zadanie \cb0dfffStun\d.</string>
	<string name="skills.attacks.mainhand.axe.name">Axe Slash</string>
	<string name="skills.attacks.mainhand.axe.desc">Rozdzierające cięcie, które zadaje 100% obrażeń od broni i ma 20% szans na zadanie stosu \cb0dfffBleeding\d.</string>
	<string name="skills.attacks.mainhand.sword.name">Sword Swing</string>
	<string name="skills.attacks.mainhand.sword.desc">Szeroki zamach po łuku zadający 100% obrażeń od broni.</string>
	<string name="skills.attacks.mainhand.dagger.name">Cięcie sztyletem</string>
	<string name="skills.attacks.mainhand.dagger.desc">Szybkie cięcie, które zadaje 100% obrażeń od broni i ma 20% szans na zadanie stosu \cb0dfffBleeding\d.</string>
	<string name="skills.attacks.mainhand.dirk.name">Dirk Stab</string>
	<string name="skills.attacks.mainhand.dirk.desc">Szybkie dźgnięcie zadające 60% obrażeń od broni.</string>
	<string name="skills.attacks.mainhand.wand.fire.name">Fire Bolt</string>
	<string name="skills.attacks.mainhand.wand.fire.desc">Wystrzeliwuje ognisty pocisk, który eksploduje przy uderzeniu, zadając 40% obrażeń od broni.</string>
	<string name="skills.attacks.mainhand.wand.ice.name">Odłamki lodu</string>
	<string name="skills.attacks.mainhand.wand.ice.desc">Uwolnij trzy odłamki lodu, z których każdy zadaje 60% obrażeń od broni.</string>
	<string name="skills.attacks.mainhand.wand.lightning.name">Lightning Bolt</string>
	<string name="skills.attacks.mainhand.wand.lightning.desc">Wyładowuje błyskawicę, która może przeskakiwać między wrogami, zadając 40% obrażeń od broni. Piorun zadaje 10% mniej obrażeń za każdy skok.</string>

	<string name="skills.attacks.offhand.crossbow.name">Szybki strzał</string>
	<string name="skills.attacks.offhand.crossbow.desc">Szybko wypuszcza szybki pocisk, który zadaje 75% obrażeń fizycznych.</string>
	<string name="skills.attacks.offhand.shield.name">Shield Bash</string>
	<string name="skills.attacks.offhand.shield.desc">Uderzaj wrogów tarczą, zadając im \cb0dfffStun\d.</string>
	<string name="skills.attacks.offhand.tome.fire.name">Glif Ognia</string>
	<string name="skills.attacks.offhand.tome.fire.desc">Umieszcza na ziemi glif, który aktywuje się, gdy wróg znajdzie się w jego pobliżu, tworząc obszar płomieni na 5 sek. i zadając 75% obrażeń od broni.</string>
	<string name="skills.attacks.offhand.tome.ice.name">Glif Lodu</string>
	<string name="skills.attacks.offhand.tome.ice.desc">Umieść na ziemi glif, który aktywuje się, gdy wróg znajdzie się w jego pobliżu, wyzwalając podmuch lodu zadający 100% obrażeń od broni na danym obszarze.</string>
	<string name="skills.attacks.offhand.tome.lightning.name">Glif Błyskawicy</string>
	<string name="skills.attacks.offhand.tome.lightning.desc">Umieść na ziemi glif, który aktywuje się, gdy wróg znajdzie się w jego pobliżu, powodując wystrzelenie trzech błyskawic, z których każda zadaje 75% obrażeń od broni.</string>

	<string name="skills.attacks.twohanded.shortbow1.name">Szybki strzał</string>
	<string name="skills.attacks.twohanded.shortbow1.desc">Szybko wystrzeliwuje strzałę, która zadaje 50% obrażeń od broni.</string>
	<string name="skills.attacks.twohanded.shortbow2.name">Power Shot</string>
	<string name="skills.attacks.twohanded.shortbow2.desc">Przytrzymaj, aby naładować potężny strzał. Zasięg, prędkość i obrażenia pocisku są zwiększane przez czas ładowania, zadając do 150% obrażeń od broni.</string>

	<string name="skills.attacks.twohanded.longbow1.name">Szybki strzał</string>
	<string name="skills.attacks.twohanded.longbow1.desc">Błyskawicznie wystrzeliwuje strzałę, która zadaje 60% obrażeń od broni.</string>
	<string name="skills.attacks.twohanded.longbow2.name">Power Shot</string>
	<string name="skills.attacks.twohanded.longbow2.desc">Przytrzymaj, aby naładować potężny strzał. Zasięg, prędkość i obrażenia pocisku są zwiększane przez czas ładowania, zadając do 150% obrażeń od broni.</string>

